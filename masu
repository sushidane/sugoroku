function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  fill(0,200,255)
  stroke(0,200,255)
  circle(200,200,80)
  fill(0) 
  circle(185,190,10)
  circle(215,190,10)
  //curve(190,195,195,200,200,205,205,210)
  strokeWeight(5)
  line(190,190,200,200)
  line(200,200,210,190)
}
