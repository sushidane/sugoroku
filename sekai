let stage =1;
let player;
let setumei;

function setup() {
  drawsyoki();
}

function drawsyoki(){
  setumei=1;
  drawplayer();
  createCanvas(400, 400);
  background(255);
}

function keyPressed(){
    if(key=='f'){
     if(setumei==1){ 
       drawsetumei();
    }}
      else if(key=='g'){
     if(setumei==2){ 
      drawsyoki();
  }}
}

function drawplayer(){
  ellipse(150,150,100);
}

function drawsetumei(){
  setumei=2;
   background(0);
}

function draw() {
  }
