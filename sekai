let stage =1;
let player;
let setumei;

function setup() {
  drawsyoki();
}

function drawsyoki(){
  setumei=1;
  drawplayer();
  createCanvas(400, 400);
  background(220);
}

function keyPressed(){
    if(key=='f'){
     if(setumei==1){ 
       drawsetumei();
    }}
  }

 function drawplayer(){
  ellipse(150,150,100);
}

function drawsetumei(){
  setumei=2;
   background(0);
  function keyPressed(){
    if(key=='f'){
      drawsyoki();
    }
  }
}

