let player=0
let player2={x:10,y:10}

function setup() {
  createCanvas(400, 400);
  background(220);
}

function keyPressed(){
  if(key==' '){
    drawsaikoro()    
  }
}

function drawsaikoro(){
  player=floor(random(1,4)) 
}

function draw() {
  console.log(player)
  if(player2.x<370){
     player2.x=player2.x+player*10
     }
  else if(player2.x>369){
          player2.x=10
          player2.y+=30
          }
  
  rect(player2.x,player2.y,20)  
}
